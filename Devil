local whitelist = {
    ["208482724jf"] = true,
    ["bestwars17"] = true,
    ["ZD_SD17"] = true
}

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

if not whitelist[LocalPlayer.Name] then
    LocalPlayer:Kick("مو مسموح لك هذا سكربت خاص)
end
-- AutoRebirth on script start (no toggle)
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local RE_EVENTS_NAME = "rEvents"
local REBIRTH_NAME = "rebirthRemote"
local INTERVAL = 0.1

local rEvents = ReplicatedStorage:WaitForChild(RE_EVENTS_NAME)
local rebirthRemote = rEvents and rEvents:WaitForChild(REBIRTH_NAME)

if not rebirthRemote then
    warn("AutoRebirth: couldn't find ".. RE_EVENTS_NAME .. "." .. REBIRTH_NAME)
    return
end

local function sendRebirth()
    local ok, err = pcall(function()
        if rebirthRemote:IsA("RemoteFunction") then
            rebirthRemote:InvokeServer("rebirthRequest")
        elseif rebirthRemote:IsA("RemoteEvent") then
            rebirthRemote:FireServer("rebirthRequest")
        else
            if typeof(rebirthRemote.InvokeServer) == "function" then
                rebirthRemote:InvokeServer("rebirthRequest")
            elseif typeof(rebirthRemote.FireServer) == "function" then
                rebirthRemote:FireServer("rebirthRequest")
            else
                error("Unknown remote type")
            end
        end
    end)
    if not ok then
        warn("AutoRebirth error:", err)
    end
end

task.spawn(function()
    while true do
        sendRebirth()
        task.wait(INTERVAL)
    end
end)
